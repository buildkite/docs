{
  "containerDefinitions": [
    {
      "name": "web",
      "image": "",
      "portMappings": [
        {
          "containerPort": 3000,
          "hostPort": 3000,
          "protocol": "tcp"
        }
      ],
      "essential": true,
      "environment": [
        {
          "name": "RAILS_LOG_TO_STDOUT",
          "value": "true"
        },
        {
          "name": "RAILS_SERVE_STATIC_FILES",
          "value": "true"
        },
        {
          "name": "BADGE_DOMAIN",
          "value": "https://badge.buildkite.com"
        }
      ],
      "secrets": [
        {
          "name": "SECRET_KEY_BASE",
          "valueFrom": "/ecs/docs/SECRET_KEY_BASE"
        },
        {
          "name": "ALGOLIA_API_KEY",
          "valueFrom": "/ecs/docs/ALGOLIA_API_KEY"
        },
        {
          "name": "ALGOLIA_APP_ID",
          "valueFrom": "/ecs/docs/ALGOLIA_APP_ID"
        },
        {
          "name": "ALGOLIA_INDEX_NAME",
          "valueFrom": "/ecs/docs/ALGOLIA_INDEX_NAME"
        },
        {
          "name": "BUGSNAG_API_KEY",
          "valueFrom": "/ecs/docs/BUGSNAG_API_KEY"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/docs",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ],
  "family": "docs",
  "executionRoleArn": "",
  "networkMode": "awsvpc",
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "cpu": "512",
  "memory": "1024"
}
